{
    "id": "org.webosports.service.filemanager",
    "description": "filemanager service",
    "activityTimeout": 3600,
    "services": [ {
        "name": "org.webosports.service.filemanager",
        "description": "filemanager service",
        "commands": [
			{
                "name": "removeDir",
                "assistant": "removeDirAssistant",
                "public": true
            },
			{
                "name": "mkDir",
                "assistant": "mkDirAssistant",
                "public": true
            },
			{
                "name": "removeFile",
                "assistant": "removeFileAssistant",
                "public": true
            },
			{
                "name": "moveFile",
                "assistant": "moveFileAssistant",
                "public": true
            },
			{
                "name": "getFileSize",
                "assistant": "getFileSizeAssistant",
                "public": true
            },
			{
                "name": "GetDirectories",
                "assistant": "GetDirectoriesAssistant",
                "public": true
            }
        ]
    } ]
}
